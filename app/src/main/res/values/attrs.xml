<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ExpandableTextView">
        <attr name="maxCollapsedLines" format="integer" />
        <attr name="animDuration" format="integer" />
        <attr name="contentTextSize" format="dimension" />
        <attr name="contentTextColor" format="color" />
        <attr name="contentLineSpacingMultiplier" format="float" />
        <attr name="expandDrawable" format="reference" />
        <attr name="expandText" format="string" />
        <attr name="collapseDrawable" format="reference" />
        <attr name="collapseText" format="string" />
        <attr name="expandCollapseTextColor" format="color" />
        <attr name="DrawableAndTextGravity">
            <enum name="left" value="0" />
            <enum name="center" value="1" />
            <enum name="right" value="2" />
        </attr>
    </declare-styleable>

    <declare-styleable name="SampleCircleImageView">
        <!-- 源图片 -->
        <attr name="src" format="reference"></attr>
        <!-- 圆角大小 -->
        <attr name="radius" format="dimension"></attr>
    </declare-styleable>

    <declare-styleable name="MarqueeView">

        <attr name="textcolor" format="color"></attr>

        <attr name="textSize" format="dimension"></attr>

        <attr name="marqueebackground" format="color"></attr>

        <attr name="isRepeat" format="boolean"></attr>

        <attr name="startPoint">
            <enum name="start" value="0" />
            <enum name="end" value="1" />
        </attr>

        <attr name="direction">
            <enum name="left" value="0" />
            <enum name="right" value="1" />
        </attr>

        <attr name="speed" format="integer"></attr>

        <attr name="marqueeInterval" format="integer" />
        <attr name="marqueeAnimDuration" format="integer" />
        <attr name="marqueeAnimIn" format="reference" />
        <attr name="marqueeAnimOut" format="reference" />

    </declare-styleable>

    <declare-styleable name="LikeLayout">
        <attr name="button_color" format="color" />
        <attr name="checked_img" format="reference" />
        <attr name="uncheck_img" format="reference" />
        <attr name="button_title" format="string" />
        <attr name="checked" format="boolean" />
        <attr name="checked_background" format="reference" />
        <attr name="unchecked_background" format="reference" />
    </declare-styleable>
